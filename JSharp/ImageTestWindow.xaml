<Window x:Class="JSharp.ImageTestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JSharp"
        mc:Ignorable="d"
        Title="ImageTestWindow" Height="450" Width="800">
    <Grid>
		<Image x:Name="imageControl" Width="255" Height="100" Stretch="UniformToFill" Margin="-2,0,50,0" Visibility="Visible" />
	</Grid>
</Window>
